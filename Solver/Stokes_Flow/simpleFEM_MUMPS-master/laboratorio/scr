grep CPU opla | grep analysis >> analysis.dat
grep CPU opla | grep factor | grep time >> factorisation.dat
grep CPU opla | grep solution | grep time >> solution.dat
grep CPU opla | grep total >> total.dat
grep eff opla | grep analysis >> eff_analysis.dat
grep eff opla | grep factor | grep time >> eff_factorisation.dat
grep eff opla | grep solution | grep time >> eff_solution.dat
grep eff opla | grep total >> eff_total.dat
grep effective opla | grep fact >> memory_factorisation.dat
grep effective opla | grep solu >> memory_solution.dat

